<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h1>Regulated Fish</h1>
      <section>
        <% @fish_with_regulations.each do |fish| %>
          <article>
            <%= link_to title(fish), fish_path(fish) %>
            <h5>Regulations</h5>
            <ul style="list-style-type: none;">
              <li>Minimum Size: <%= fish.minimum_size %></li>
              <li>Bag Limit: <%= fish.bag_limit %></li>
            </ul>
          </article>
        <% end %>
      </section>
    </div>
  </div>
</div>
